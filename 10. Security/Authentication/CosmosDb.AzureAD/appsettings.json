{
    // Notice there is no master key... it is never needed with RBAC using Azure AD

    "CosmosEndpoint": "[ENDPOINT]",

    // Both applications require the Azure AD directory ID (aka tenant ID)

    "AadDirectoryId": "[DIRECTORY-ID]",

    // The Ingest-Only application also requires the client ID and secret defined for it

    "AadIngestOnlyClientId": "[CLIENT-ID]",
    "AadIngestOnlyClientSecret": "[CLIENT-SECRET]",

    // The Read-Only application also requires the client ID and secret defined for it

    "AadReadOnlyClientId": "[CLIENT-ID]",
    "AadReadOnlyClientSecret": "[CLIENT-SECRET]"
}
